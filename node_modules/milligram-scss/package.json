  "dependencies": {
    "normalize.css": "latest"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "node-sass": "^3.7.0",
    "npm-run-all": "^2.1.1",
    "onchange": "^2.4.0",
    "postcss-cli": "^2.5.2",
    "sass-lint": "^1.8.2"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "scripts": {
    "prestart": "npm install",
    "pretest": "npm install",
    "copy": "rm -rf dist && mkdir dist && cp -rf src/* dist",
    "banner": "for m in dist/*.scss; do echo '//\n// Milligram v1.2.0\n// http://milligram.github.io\n//\n// Copyright (c) 2016 CJ Patoilo\n// Licensed under the MIT license\n//\n' | cat - $m > temp && mv temp $m; done",
    "autoprefixer": "postcss -u autoprefixer --no-map.inline --autoprefixer.browsers 'last 1 versions' -r test/*.css",
    "scss": "node-sass --output-style expanded src/milligram.scss test/milligram.css && node-sass --output-style compressed src/milligram.scss test/milligram.min.css",
    "lint": "sass-lint -c .sasslintrc src/*.scss --verbose --no-exit",
    "build": "run-s lint scss autoprefixer copy banner",
    "watch": "onchange src -- npm run build",
    "start": "run-p watch",
    "test": "run-s build"
  },
